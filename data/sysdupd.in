#!/usr/bin/python3
import os
import sys

# meson replaces these during install
sys.path.insert(1, '@python_dir@')
os.environ['SYSDUPD_PKGDATADIR'] = '@pkgdatadir@'

if __name__ == "__main__":
    # if kept the folder named 'src' in site-packages:
    from sysdupd.__main__ import main
    sys.exit(main())